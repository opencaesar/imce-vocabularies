vocabulary <http://iso.org/80000-3/quantityKinds> with # as quantityKinds {
    extends <http://iso.org/80000-3/units>

    @dc:source "3-1.1 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept ^length :> metrology:IndependentUnitaryQuantityKind [
        restricts all relation metrology:hasMeasurementUnit to units:Metre
    ]
    
    @dc:source "3-1.2 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept width :> ^length

    @dc:source "3-1.2 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept breadth :> width

    @dc:source "3-1.3 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept height :> ^length

    @dc:source "3-1.3 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept depth :> height

    @dc:source "3-1.3 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept altitude :> height

    @dc:source "3-1.4 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept thickness :> ^length

    @dc:source "3-1.5 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept diameter :> ^length

    @dc:source "3-1.6 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept radius :> ^length

    @dc:source "3-1.7 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept path-length :> ^length

    @dc:source "3-1.7 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept arc-length :> path-length

    @dc:source "3-1.8 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept distance :> ^length

    @dc:source "3-1.9 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept radial-distance :> ^length

    @dc:source "3-1.10 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept position-vector :> ^length

    @dc:source "3-1.11 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept displacement :> ^length

    @dc:source "3-1.12 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept radius-of-curvature :> ^length

    @dc:source "3-7 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept time :> metrology:IndependentUnitaryQuantityKind [
        restricts all relation metrology:hasMeasurementUnit to units:Second
    ]

    @dc:source "3-8 [https://www.iso.org/obp/ui/#iso:std:iso:80000:-3:ed-2:v1:en]"
    concept velocity :> metrology:IndependentUnitaryQuantityKind [
        restricts all relation metrology:hasMeasurementUnit to units:Metre-per-Second
    ]

    concept speed :> velocity
}